{
  "name": "interviewor",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "generate": "npm -s run generate:prisma && npm -s run generate:types",
    "generate:prisma": "prisma generate",
    "generate:types": "graphql-codegen --config codegen.ts",
    "migrate:dev": "prisma migrate dev --name init",
    "prettier:check": "prettier --check \"./src/**/*.{ts,tsx,js,jsx,md}\"",
    "prettier:format": "prettier --write \"./src/**/*.{ts,tsx,js,jsx,md}\"",
    "seed": "ts-node ./src/server/prisma/seed.ts",
    "scrape": "ts-node ./scraper/index.ts",
    "prepare": "husky install",
    "postinstall": "prisma generate || true"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/server": "^11.10.0",
    "@liveblocks/client": "^0.18.1",
    "@liveblocks/react": "^0.18.1",
    "@mantine/carousel": "^5.3.3",
    "@mantine/core": "^5.3.0",
    "@mantine/form": "^5.3.0",
    "@mantine/hooks": "^5.3.0",
    "@mantine/modals": "^5.3.0",
    "@mantine/next": "^5.3.0",
    "@mantine/nprogress": "^5.3.0",
    "@primer/octicons-react": "^17.5.0",
    "@prisma/client": "^4.3.1",
    "@svgr/webpack": "^6.3.1",
    "@tanstack/react-query": "^4.3.4",
    "apollo-server-micro": "^3.10.2",
    "class-validator": "^0.13.2",
    "color2k": "^2.0.0",
    "embla-carousel-autoplay": "^7.0.3",
    "embla-carousel-react": "^7.0.3",
    "framer-motion": "^7.3.5",
    "graphql": "^15.3.0",
    "graphql-request": "^5.0.0",
    "micro": "^9.4.1",
    "next": "^12.3.1",
    "next-auth": "^4.10.3",
    "next-seo": "^5.5.0",
    "nextjs-progressbar": "^0.0.14",
    "node-fetch": "^2.6.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intersection-observer": "^9.4.0",
    "reflect-metadata": "^0.1.13",
    "spacetime": "^7.2.0",
    "type-graphql": "^1.1.1",
    "zod": "^3.18.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.12.0",
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-graphql-request": "^4.5.4",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@graphql-codegen/typescript-react-query": "^4.0.1",
    "@types/node": "18.0.0",
    "@types/node-fetch": "^2.6.2",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "autoprefixer": "^10.4.10",
    "eslint": "8.22.0",
    "eslint-config-next": "12.2.5",
    "husky": "^8.0.1",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prisma": "^4.3.1",
    "puppeteer": "^17.1.3",
